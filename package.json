{
  "name": "wiki-reliability",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n server,frontend -c auto \"tsx watch src/index.ts\" \"npm run dev --prefix src/frontend\"",
    "build": "tsc -p tsconfig.json && npm run build --prefix src/frontend",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "bs": "npm run build && npm run start",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "concurrently": "^9.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.6.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
